pipeline{
   agent any
        stages{
               stage("build"){
                   steps{
                       echo"build successfull"
                   }
               }
               stage("test"){
                   steps{
                       echo"test successfull"
                   }
               }

               stage("deploy"){
                   steps{
                    echo"deploy sucessfull"
                    }
               }
        }
 }
